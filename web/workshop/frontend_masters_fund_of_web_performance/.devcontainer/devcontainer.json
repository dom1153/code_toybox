{
	"name": "Node.js",
	"image": "mcr.microsoft.com/devcontainers/javascript-node:1-22-bookworm",
	"postCreateCommand": "npm install",
	"portsAttributes": {
		"3000": {
			"label": "Port Forward for CLI",
			"onAutoForward": "notify"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"bradlc.vscode-tailwindcss",
				"esbenp.prettier-vscode"
			]
		}
	}
}
